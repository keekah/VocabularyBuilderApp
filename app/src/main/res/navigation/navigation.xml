<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation.xml"
    app:startDestination="@id/fragment_word_list">

    <fragment
        android:id="@+id/fragment_word_list"
        android:name="me.wingert.vocabularybuilder.WordListFragment"
        android:label="WordListFragment"
        tools:layout="@layout/fragment_word_list">
        <action
            android:id="@+id/action_fragment_word_list_to_undefinedWordsFragment"
            app:destination="@id/fragment_undefined_words" />
    </fragment>
    <fragment
        android:id="@+id/fragment_undefined_words"
        android:name="me.wingert.vocabularybuilder.UndefinedWordsFragment"
        android:label="UndefinedWordsFragment"
        tools:layout="@layout/fragment_undefined_words" />

</navigation>